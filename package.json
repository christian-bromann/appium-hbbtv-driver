{
  "name": "appium-hbbtv-driver",
  "author": "Christian Bromann <christian@saucelabs.com>",
  "description": "Appium driver for automating HbbTV ready devices",
  "version": "0.0.0",
  "keywords": [
    "appium",
    "hbbtv"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://gitlab.fokus.fraunhofer.de/christian.bromann/appium-hbbtv-driver.git"
  },
  "bugs": {
    "url": "https://gitlab.fokus.fraunhofer.de/christian.bromann/appium-hbbtv-driver/issues"
  },
  "engines": [
    "node"
  ],
  "main": "./build/index.js",
  "bin": {},
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "run-s clean compile webpack",
    "clean": "rm -rf ./build",
    "compile": "babel lib/ -d build/ --ignore lib/webdriver,lib/debugger/",
    "dev": "run-p start:dev:*",
    "eslint": "eslint ./lib/*.js",
    "start": "npm run start:server",
    "start:dev": "nodemon -e js,json,yaml --watch build --ignore build/webdriver/ --ignore build/debugger/",
    "start:server": "node build/index.js",
    "start:dev:babel": "npm run compile -- --watch",
    "start:dev:webpack": "npm run webpack -- --watch",
    "test": "run-s eslint test:unit test:e2e",
    "test:unit": "mocha --compilers js:babel-core/register --colors --reporter dot test/**/*.spec.js",
    "test:unit:cover": "babel-node ./node_modules/.bin/isparta cover --include './lib/**/*.js' _mocha",
    "test:e2e": "echo 'no e2e tests yet'",
    "webpack": "webpack --progress --colors --config config/webpack.config"
  },
  "dependencies": {
    "appium-base-driver": "~2.3.0",
    "appium-support": "~2.5.1",
    "babel-runtime": "~6.23.0",
    "body-parser": "~1.17.0",
    "cookie-parser": "~1.4.3",
    "express": "~4.15.0",
    "js-cookie": "~2.1.3",
    "json-stringify-safe": "~5.0.1",
    "mime-types": "~2.1.14",
    "request-promise-native": "~1.0.3",
    "serve-favicon": "~2.4.1",
    "socket.io": "~1.7.2",
    "tmp": "~0.0.31",
    "uuid": "~3.0.1",
    "validator": "~7.0.0",
    "virtualbox-promise": "~0.1.0",
    "ws": "~2.2.0",
    "xml2js": "~0.4.17",
    "yargs": "~6.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "3.5.0",
    "eslint": "^3.14.1",
    "eslint-config-standard": "^7.0.0",
    "eslint-plugin-mocha": "4.8.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "isparta": "4.0.0",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.1",
    "webpack": "^2.2.1",
    "webpack-combine-loaders": "^2.0.3",
    "webpack-dev-server": "^2.2.1",
    "webpack-merge": "^3.0.0",
    "webpack-validator": "^2.3.0"
  }
}
