{
  "name": "appium-hbbtv-driver",
  "author": "Christian Bromann <christian@saucelabs.com>",
  "description": "Appium driver for automating HbbTV ready devices",
  "version": "0.0.0",
  "keywords": [
    "appium",
    "hbbtv"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://gitlab.fokus.fraunhofer.de/christian.bromann/appium-hbbtv-driver.git"
  },
  "bugs": {
    "url": "https://gitlab.fokus.fraunhofer.de/christian.bromann/appium-hbbtv-driver/issues"
  },
  "engines": [
    "node"
  ],
  "main": "./build/index.js",
  "bin": {},
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "npm-run-all clean compile",
    "clean": "rm -rf ./build",
    "compile": "babel lib/ -d build/ --ignore lib/driver/",
    "dev": "npm-run-all compile --parallel start:*",
    "eslint": "eslint ./lib/*.js",
    "start": "npm run start:server",
    "start:babel": "npm run compile -- --watch",
    "start:server": "node build/index.js",
    "start:webpack": "npm run webpack -- --watch",
    "webpack": "webpack --progress --colors --config config/webpack.config"
  },
  "dependencies": {
    "appium-base-driver": "^2.0.12",
    "appium-logger": "^2.1.0",
    "babel-runtime": "^6.11.6",
    "json-stringify-safe": "^5.0.1",
    "lodash.topairs": "^4.2.0",
    "request-promise-native": "^1.0.2",
    "socket.io": "^1.4.8",
    "tmp": "0.0.28",
    "uuid": "^2.0.2",
    "validator": "^5.5.0",
    "virtualbox-promise": "^0.1.0",
    "xml2js": "^0.4.17",
    "yargs": "^4.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.11.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "load-grunt-tasks": "^3.5.0",
    "webpack": "2.2.1",
    "webpack-combine-loaders": "2.0.3",
    "webpack-dev-server": "2.2.1",
    "webpack-merge": "2.6.1",
    "webpack-validator": "2.3.0"
  }
}
